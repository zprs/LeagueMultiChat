{
  "name": "LeagueMultiChat",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/leagueappicon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/leagueappicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"League Multi Chat\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/leagueappicon.png --prune=true --out=release-builds",
    "rebuild-all": "npm rebuild && electron-rebuild -v 12.0.2",
    "fix-windows-type-lag": "copy \"./keypress.c\" \"./node_modules/robotjs/src\" /y && npm run rebuild-all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "robotjs": "^0.6.0",
    "socket.io": "^4.0.1"
  }
}
